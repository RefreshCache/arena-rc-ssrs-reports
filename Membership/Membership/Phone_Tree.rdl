        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT
lookup_id
, lookup_value
FROM
core_lookup
WHERE
lookup_type_id=35</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
