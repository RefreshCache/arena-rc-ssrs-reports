    <DataSet Name="Contact_Info">
      <Fields>
        <Field Name="family_id">
          <DataField>family_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="family_name">
          <DataField>family_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="person_id">
          <DataField>person_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nick_name">
          <DataField>nick_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="email">
          <DataField>email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        core_family_member.family_id, core_family.family_name, core_person.person_id, core_person.nick_name, core_person.last_name, core_person_email.email
FROM            core_person INNER JOIN
                         core_person_email ON core_person.person_id = core_person_email.person_id INNER JOIN
                         core_family_member ON core_person.person_id = core_family_member.person_id INNER JOIN
                         core_family ON core_family_member.family_id = core_family.family_id INNER JOIN
                         core_lookup ON core_person.member_status = core_lookup.lookup_id
WHERE        (core_person_email.email_order = 0) AND (core_person.record_status IN (@RecStatus)) AND (core_lookup.lookup_id IN (@MemStatus))
ORDER BY core_family.family_name, core_family.family_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RecStatus">
            <Value>=Parameters!RecStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MemStatus">
            <Value>=Parameters!MemStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Member_Status">
      <Fields>
        <Field Name="lookup_id">
          <DataField>lookup_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="lookup_value">
          <DataField>lookup_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        core_lookup.lookup_id, core_lookup.lookup_value
FROM            core_lookup INNER JOIN
                         core_lookup_type ON core_lookup.lookup_type_id = core_lookup_type.lookup_type_id
WHERE        (core_lookup_type.lookup_type_name = 'Membership Status') AND (core_lookup.active = 1)
ORDER BY core_lookup.lookup_value</CommandText>
      </Query>
    </DataSet>
  </DataSets>
