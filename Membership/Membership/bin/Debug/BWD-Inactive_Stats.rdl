      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>select cp.person_id, cp.nick_name, cp.last_name, cp.record_status, cl.lookup_value, cp.date_modified, cpa.datetime_value
from core_person cp
left join core_lookup cl on cp.inactive_reason_luid = cl.lookup_id
left join core_person_attribute cpa on cp.person_id = cpa.person_id and cpa.attribute_id = 407
where cp.record_status = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
