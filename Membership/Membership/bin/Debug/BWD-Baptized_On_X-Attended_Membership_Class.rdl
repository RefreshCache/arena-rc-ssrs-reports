  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Person_ID">
          <DataField>Person_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Person_Name">
          <DataField>Person_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Member_Status">
          <DataField>Member_Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Birth_Date">
          <DataField>Birth_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Membership_Class_Date">
          <DataField>Membership_Class_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        cp.person_id AS Person_ID, cp.nick_name + ' ' + cp.last_name AS Person_Name, cl.lookup_value AS Member_Status, cp.birth_date AS Birth_Date, 
                         cpa.datetime_value AS Membership_Class_Date
FROM            core_person_attribute AS cpa INNER JOIN
                         core_person AS cp ON cpa.person_id = cp.person_id INNER JOIN
                         core_lookup AS cl ON cp.member_status = cl.lookup_id
WHERE        (cpa.person_id IN
                             (SELECT        person_id
                               FROM            core_person_attribute
                               WHERE        (attribute_id = 19) AND (datetime_value = @BaptDate))) AND (cpa.attribute_id = 159)
ORDER BY cp.last_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BaptDate">
            <Value>=Parameters!BaptDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
