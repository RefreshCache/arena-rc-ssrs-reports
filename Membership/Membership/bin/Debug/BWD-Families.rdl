  <DataSources>
    <DataSource Name="ArenaDB">
      <DataSourceReference>ArenaDB</DataSourceReference>
      <rd:DataSourceID>df0ca6de-57b6-495b-806c-43dfaf253cc7</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="AllFamilies">
      <Fields>
        <Field Name="family_id">
          <DataField>family_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="family_name">
          <DataField>family_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        cf.family_id, cf.family_name
FROM            core_family AS cf INNER JOIN
                         core_family_member AS cfm ON cf.family_id = cfm.family_id INNER JOIN
                         core_person AS cp ON cfm.person_id = cp.person_id
GROUP BY cf.family_id, cf.family_name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="ActiveFamilies">
      <Fields>
        <Field Name="family_id">
          <DataField>family_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="family_name">
          <DataField>family_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        cf.family_id, cf.family_name
FROM            core_family AS cf INNER JOIN
                         core_family_member AS cfm ON cf.family_id = cfm.family_id INNER JOIN
                         core_person AS cp ON cfm.person_id = cp.person_id
GROUP BY cf.family_id, cf.family_name, cp.record_status, cfm.role_luid
HAVING        (cp.record_status = 0) AND (cfm.role_luid = 29)</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="AllIndividuals">
      <Fields>
        <Field Name="person_id">
          <DataField>person_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        person_id
FROM            core_person</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="ActiveIndividuals">
      <Fields>
        <Field Name="person_id">
          <DataField>person_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="record_status">
          <DataField>record_status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        person_id, record_status
FROM            core_person
WHERE        (record_status = 0)</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="FamilyStats">
      <Fields>
        <Field Name="family_id">
          <DataField>family_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="total_members">
          <DataField>total_members</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        family_id, COUNT(family_id) AS total_members
FROM            core_family_member
GROUP BY family_id</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="FamiliesOver18">
      <Fields>
        <Field Name="family_id">
          <DataField>family_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT cfm.family_id
FROM
	core_family_member cfm
WHERE
	cfm.person_id IN
		(
			SELECT
				cp.person_id
			FROM
				core_person cp
			WHERE
				cp.birth_date &lt; (GETDATE() - YEAR(18))
				AND cp.record_status = 0
		)
GROUP BY cfm.family_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
