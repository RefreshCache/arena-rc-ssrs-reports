      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT person_id, active_meter, date_created
FROM core_active_meter_history
WHERE person_id = @PersonID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@PersonID">
            <Value>=Parameters!PersonID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>ArenaDB</DataSourceName>
      </Query>
