  <DataSets>
    <DataSet Name="Status_Summary">
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        core_lookup.lookup_value AS Status, COUNT(*) AS Total
FROM            core_person INNER JOIN
                         core_lookup ON core_person.member_status = core_lookup.lookup_id
GROUP BY core_lookup.lookup_value, core_person.record_status
HAVING        (core_person.record_status IN (@RecStatus))
ORDER BY Total DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RecStatus">
            <Value>=Parameters!RecStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
