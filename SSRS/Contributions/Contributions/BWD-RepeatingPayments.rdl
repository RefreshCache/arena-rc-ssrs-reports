    <DataSet Name="RepeatingPayments">
      <Fields>
        <Field Name="transaction_detail">
          <DataField>transaction_detail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="payment_frequency">
          <DataField>payment_frequency</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="first_payment_date">
          <DataField>first_payment_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="num_of_payments">
          <DataField>num_of_payments</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="lookup_value">
          <DataField>lookup_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="active">
          <DataField>active</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="payment_amount">
          <DataField>payment_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="date_created">
          <DataField>date_created</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="email">
          <DataField>email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="first_name">
          <DataField>first_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        prp.transaction_detail, cp.last_name + ', ' + cp.first_name AS name, prp.payment_frequency, prp.first_payment_date, prp.num_of_payments, cl.lookup_value, 
                         prp.active, prp.payment_amount, prp.date_created, cpe.email, cp.first_name, cp.last_name
FROM            pmnt_repeating_payment AS prp INNER JOIN
                         core_person AS cp ON prp.person_id = cp.person_id INNER JOIN
                         core_lookup AS cl ON prp.payment_method_luid = cl.lookup_id LEFT OUTER JOIN
                         core_person_email AS cpe ON prp.person_id = cpe.person_id AND cpe.email_order = '0'
WHERE        (prp.date_created BETWEEN @StartDate AND @EndDateMod)
ORDER BY prp.date_created DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDateMod">
            <Value>=Parameters!EndDateMod.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
