  <Author>Scott</Author>
  <Code>Public Shared Function SupressNegative(ByVal value As Decimal)

	If value &lt; 0 Then
		SupressNegative = ""
	Else
		SupressNegative =  FormatCurrency(value, 2)
	End If

End Function

Shared offset as Integer
Shared currentgroup as Object

Public Function GetGroupPageNumber(group as Object, pagenumber as Integer) as Object
  If (currentgroup = String.Empty)
	currentgroup = group
  End If
  If Not (group = currentgroup)
   offset = pagenumber - 1
   currentgroup = group  
  End If

  IF (pagenumber - offset &gt; 1)
	return "Page " +  Format(pagenumber - offset)
  ELSE
	return "Page " +  Format(pagenumber - offset)
  End If  
End Function

Shared currentStatment as Object

Public Function GetContribStatement(givingStatement as Object) as Object
	If (currentStatment = String.empty)
		currentStatment = givingStatement
	End If
	return currentStatment 
End Function

Shared currentOrgUrl as Object
Public Function GetOrgUrl(url as Object) as Object
	if (currentOrgUrl = String.empty)
		currentOrgUrl = url
	End If
	return currentOrgUrl
End Function

Shared currentPerson as Object
Public Function GetPerson(person as Object) as Object
	if (currentPerson = String.empty)
		currentPerson = person
	End If
	if not (currentPerson = person)
		if not (person = String.empty)
			currentPerson = person
		end if
	End if
	return currentPerson
End Function</Code>
