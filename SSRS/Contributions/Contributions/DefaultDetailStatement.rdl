      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT     C.contribution_id, C.contribution_date, C.person_id, C.batch_id, C.currency_amount, CF.amount, F.organization_id, F.fund_name, F.fund_description, F.online_name, F.active, F.fund_id, F.tax_deductible, PV.first_name, PV.last_name, PV.street_address_1, PV.street_address_2, PV.city, PV.state, PV.postal_code, P.giving_unit_id, P.envelope_number, C.transaction_number, C.memo, SPOUSE.first_name as "s_first_name", SPOUSE.last_name as "s_last_name", SPOUSE.giving_unit_id as "s_giving_unit"
FROM         ctrb_contribution AS C INNER JOIN
                      ctrb_contribution_fund AS CF ON C.contribution_id = CF.contribution_id INNER JOIN
                      ctrb_fund AS F ON CF.fund_id = F.fund_id INNER JOIN
                      core_v_person_basic AS PV ON dbo.ctrb_funct_givingHead(C.person_id) = PV.person_id INNER JOIN
                      core_person AS P ON PV.person_id = P.person_id
                      LEFT JOIN core_person as SPOUSE
                      on SPOUSE.person_id = dbo.core_funct_spouse(PV.person_id)
WHERE    ((C.contribution_date &gt;= @FromDate) AND (C.contribution_date &lt;= @ToDate)
                 AND (@IncludeTaxDeductibleFunds = 0 OR F.tax_deductible = 1) 
                 AND (@PersonId IS NULL OR P.person_id = @PersonId)) AND (CF.amount &gt; 0) AND (PV.last_name LIKE '[' + @FromLetter + '-' + @ToLetter + ']%')

ORDER BY C.contribution_date, F.fund_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeTaxDeductibleFunds">
