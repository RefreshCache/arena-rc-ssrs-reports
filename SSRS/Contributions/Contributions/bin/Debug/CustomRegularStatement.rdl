      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT *, A.street_address_1 +
		CASE WHEN A.street_address_2 IS NOT NULL AND A.street_address_2 &lt;&gt; '' THEN
			CHAR(10) + CHAR(13) + A.street_address_2
		ELSE '' END AS address
FROM
orgn_organization o
JOIN core_address a ON a.address_id = o.address_id
WHERE o.organization_id = @OrgId</CommandText>
