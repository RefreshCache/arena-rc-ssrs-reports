        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        cp.nick_name, cp.first_name, cp.middle_name, cp.last_name, cp.birth_date, coa.check_in_time, coa.check_out_time, co.occurrence_name, 
                         co.occurrence_description
FROM            core_person AS cp INNER JOIN
                         core_occurrence_attendance AS coa ON cp.person_id = coa.person_id INNER JOIN
                         core_occurrence AS co ON coa.occurrence_id = co.occurrence_id
WHERE        (coa.attended = '1') AND (cp.last_name IN (@Lname)) AND (cp.first_name IN (@Fname)) AND (cp.birth_date IN (@Bday))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Lname">
            <Value>=Parameters!Lname.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Fname">
            <Value>=Parameters!Fname.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Bday">
            <Value>=Parameters!Bday.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
