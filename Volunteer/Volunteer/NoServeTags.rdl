  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="person_id">
          <DataField>person_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="person_name">
          <DataField>person_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="nick_name">
          <DataField>nick_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="birth_date">
          <DataField>birth_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT DISTINCT 
                         core_person.person_id, core_person.nick_name + ' ' + core_person.last_name AS person_name, core_person.nick_name, core_person.last_name, 
                         core_person.birth_date
FROM            core_person INNER JOIN
                         core_profile_member ON core_person.person_id = core_profile_member.person_id INNER JOIN
                         core_profile ON core_profile_member.profile_id = core_profile.profile_id INNER JOIN
                         core_lookup ON core_profile_member.status_luid = core_lookup.lookup_id
WHERE        (core_person.person_id NOT IN
                             (SELECT        person_id
                               FROM            core_profile_member AS cpm
                               WHERE        (profile_id IN
                                                             (SELECT        profile_id
                                                               FROM            core_profile AS cp2
                                                               WHERE        (profile_type = 2))))) AND (core_person.birth_date &lt; @bday) AND (core_person.member_status IN (@mstatus)) AND 
                         (core_person.record_status IN (@rstatus))
ORDER BY core_person.last_name, core_person.nick_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@bday">
            <Value>=Parameters!bday.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@mstatus">
            <Value>=Parameters!mstatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@rstatus">
            <Value>=Parameters!rstatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="MemberStatus">
      <Fields>
        <Field Name="member_status">
          <DataField>member_status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="lookup_value">
          <DataField>lookup_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT DISTINCT core_person.member_status, core_lookup.lookup_value
FROM            core_person INNER JOIN
                         core_lookup ON core_person.member_status = core_lookup.lookup_id</CommandText>
      </Query>
    </DataSet>
  </DataSets>
