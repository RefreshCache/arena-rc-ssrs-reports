  <DataSets>
    <DataSet Name="MainSource">
      <Fields>
        <Field Name="person_id">
          <DataField>person_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="first_name">
          <DataField>first_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="full_name">
          <DataField>full_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>DECLARE @top int
SET                @top = @topN
                             SELECT        TOP (@top) h1.person_id, p.first_name, p.last_name, p.first_name + ' ' + p.last_name AS full_name, SUM(ccf.amount) 
                                                       AS amount
                              FROM            ctrb_contribution h1 WITH (NOLOCK) INNER JOIN
                                                       ctrb_contribution_fund AS ccf WITH (NOLOCK) ON ccf.contribution_id = h1.contribution_id AND ccf.fund_id IN (@fund) INNER JOIN
                                                       core_person p WITH (NOLOCK) ON p.person_id = h1.person_id
                              WHERE        h1.contribution_date BETWEEN @startDate AND @endDate
                              GROUP BY h1.person_id, p.last_name, p.first_name
                              HAVING         SUM(ccf.amount) BETWEEN @min AND @max
                              ORDER BY SUM(ccf.amount) DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@topN">
            <Value>=Parameters!topN.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fund">
            <Value>=Parameters!fund.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@min">
            <Value>=Parameters!min.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@max">
            <Value>=Parameters!max.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="FundSource">
      <Fields>
        <Field Name="fund_id">
          <DataField>fund_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="fund_name">
          <DataField>fund_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT fund_id,fund_name
FROM ctrb_fund WITH(NOLOCK)
WHERE active=1
ORDER BY fund_order ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
