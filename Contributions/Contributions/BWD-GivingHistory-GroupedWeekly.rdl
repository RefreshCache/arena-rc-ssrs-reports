      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        core_family.family_id, core_family.family_name, core_address.street_address_1, ctrb_contribution.currency_amount, 
                         ctrb_contribution.contribution_date
FROM            ctrb_contribution INNER JOIN
                         core_person ON ctrb_contribution.person_id = core_person.person_id INNER JOIN
                         ctrb_batch ON ctrb_contribution.batch_id = ctrb_batch.batch_id INNER JOIN
                         core_family_member ON core_person.person_id = core_family_member.person_id INNER JOIN
                         core_family ON core_family_member.family_id = core_family.family_id INNER JOIN
                         core_person_address ON core_person.person_id = core_person_address.person_id INNER JOIN
                         core_address ON core_person_address.address_id = core_address.address_id
WHERE        (ctrb_batch.batch_name LIKE '%') AND (core_person_address.primary_address = 1) AND (ctrb_contribution.contribution_date BETWEEN @StartDate AND 
                         @EndDate)
ORDER BY ctrb_contribution.contribution_date</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
