  <Code>Public Shared Function GetPN(ID As Integer) As Integer
	If IsNothing(UniqueID)
		UniqueID = -1
	End If
	If IsNothing(CurrentPage)
		CurrentPage = 1
	End If

	Dim offset
	If UniqueID &gt; 1 AND UniqueID = ID Then
		CurrentPage = CurrentPage + 1
	ElseIf UniqueID &gt; 1 and UniqueId &lt;&gt; ID Then
		CurrentPage = 1
	End If
	UniqueID = ID
	Return CurrentPage
End Function

Shared UniqueID As Integer
Shared CurrentPage As Integer</Code>
