        <CommandText>/*Set up family_funds table, which stores those funds people gave to within the specified period*/
CREATE TABLE #family_funds
(
	family_id INT NULL
	, fund_id INT NULL
	, fund_name VARCHAR(50) NULL
	, fund_order INT NULL
);
	
/*Populate #family_funds table with fund ids given to in period for families giving together*/
INSERT INTO [#family_funds] (
	[family_id],
	[fund_id],
	[fund_name],
	[fund_order]
)
SELECT DISTINCT 
	cfm.[family_id]
	,ccf.[fund_id]
	,[fund_name] = cf.[online_name]
	,cf.[fund_order]
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN [ctrb_contribution] cc ON cp.[person_id] = cc.[person_id]
JOIN [ctrb_contribution_fund] ccf ON cc.[contribution_id] = ccf.[contribution_id]
JOIN [ctrb_fund] cf ON cf.[fund_id] = ccf.[fund_id]
WHERE cp.[contribute_individually] = 0
	AND cfm.[family_id] IN (SELECT family_id FROM [core_family_member] WHERE person_id = @personId)
	AND YEAR(cc.[contribution_date]) = YEAR(@endDate) 
	AND cc.[contribution_date] &lt; @endDate
	AND cf.[tax_deductible] = 1
UNION
SELECT DISTINCT 
	cfm.[family_id]
	,cpl.[fund_id]
	,[fund_name] = cf.[online_name]
	,cf.[fund_order]
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN [ctrb_pledge] cpl ON cp.[person_id] = cpl.[person_id]
JOIN [ctrb_fund] cf ON cf.[fund_id] = cpl.[fund_id]
WHERE cp.[contribute_individually] = 0
	AND cfm.[family_id] IN (SELECT family_id FROM [core_family_member] WHERE person_id = @personId)
	AND YEAR(cpl.[pledge_begin_date]) = YEAR(@endDate)
	AND cf.[tax_deductible] = 1
ORDER BY [family_id], [fund_id]

/*Populate #individual_funds table with fund ids given to in period for family members giving individually*/	
SELECT DISTINCT 
	cp.[person_id]
	,ccf.[fund_id]
	,[fund_name] = cf.[online_name]
	,cf.[fund_order]
INTO #individual_funds
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN [ctrb_contribution] cc ON cp.[person_id] = cc.[person_id]
JOIN [ctrb_contribution_fund] ccf ON cc.[contribution_id] = ccf.[contribution_id]
JOIN [ctrb_fund] cf ON cf.[fund_id] = ccf.[fund_id]
WHERE cp.[contribute_individually] = 1
	AND YEAR(cc.[contribution_date]) = YEAR(@endDate) 
	AND cc.[contribution_date] &lt; @endDate
	AND cp.[person_id] = @personId
	AND cf.[tax_deductible] = 1
UNION
SELECT DISTINCT 
	cp.[person_id]
	,cpl.[fund_id]
	,[fund_name] = cf.[online_name]
	,cf.[fund_order]
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN [ctrb_pledge] cpl ON cp.[person_id] = cpl.[person_id]
JOIN [ctrb_fund] cf ON cf.[fund_id] = cpl.[fund_id]
WHERE cp.[contribute_individually] = 1
	AND YEAR(cpl.[pledge_begin_date]) = YEAR(@startDate)
	AND cp.[person_id] = @personId
	AND cf.[tax_deductible] = 1
ORDER BY [person_id], [fund_id]

/*Populate #family_giving table with contributions and pledges by families contributing together*/	
SELECT distinct
	ff.family_id
	, [person_id] = dbo.cust_cotr_fn_givingHeadByFamilyID(ff.family_id)
	, ff.fund_id
	, ff.fund_name
	, ff.fund_order
	, [selected_fund] = CASE WHEN ff.[fund_id] IN (@funds) THEN 1 ELSE 0 END
	, [period] = CASE WHEN cn.contribution_date BETWEEN @startDate AND @endDate THEN 0
					WHEN YEAR(cn.contribution_date) &lt; YEAR(@startDate) THEN 2
					WHEN cn.contribution_date &lt; @startDate THEN 1
					END
	, cn.[contribution_id],
		 cn.[contribution_date],
		 [currency_type] = (SELECT TOP 1 lookup_value FROM [core_lookup] WHERE lookup_id = cn.[currency_type_luid]),
		 cn.[contribution_amount],
		 cn.[transaction_number],
		 cn.[transaction_id],
		 pl.[pledge_amount]
INTO #family_giving
FROM #family_funds ff
LEFT JOIN (
	SELECT 
		cfm.[family_id]
		,cc.[contribution_id]
		,cc.[contribution_date]
		,cc.[batch_id]
		,cc.[currency_type_luid]
		,cc.[account_number]
		,cc.[transaction_number]
		,cc.[transaction_id]
		, ccf.[fund_id]
		, [contribution_amount] = ccf.[amount]
		, ccf.[non_cash]
	FROM [ctrb_contribution] cc
	JOIN [core_person] cp ON cc.[person_id] = cp.[person_id]
	JOIN [core_family_member] cfm ON cc.[person_id] = cfm.[person_id]
	JOIN [ctrb_contribution_fund] ccf ON cc.[contribution_id] = ccf.[contribution_id]
	JOIN [#family_funds] ff ON cfm.[family_id] = ff.[family_id] AND ff.[fund_id] = ccf.[fund_id]
	WHERE cc.[contribution_date] &lt;= @endDate
		AND cp.[contribute_individually] = 0
) AS cn ON cn.[family_id] = ff.[family_id] AND cn.[fund_id] = ff.[fund_id]
LEFT JOIN (
	SELECT 
		cfm2.[family_id], cpl.[fund_id], [pledge_amount] = SUM(cpl.[amount])
	FROM [ctrb_pledge] cpl
	JOIN [core_person] cp2 ON cpl.[person_id] = cp2.[person_id]
	JOIN [core_family_member] cfm2 ON cfm2.[person_id] = cp2.[person_id]
	JOIN [#family_funds] ff ON cfm2.[family_id] = ff.[family_id] AND ff.[fund_id] = cpl.[fund_id]
	WHERE cp2.[contribute_individually] = 0
	GROUP BY cfm2.[family_id], cpl.[fund_id]
) AS pl ON pl.[family_id] = ff.[family_id] AND pl.fund_id = ff.[fund_id]

/*Populate contributions and pledges by family members giving individually*/	
SELECT distinct
	ff.person_id
	, ff.fund_id
	, ff.fund_name
	, ff.fund_order
	, [selected_fund] = CASE WHEN ff.[fund_id] IN (@funds) THEN 1 ELSE 0 END
	, [period] = CASE WHEN cn.contribution_date BETWEEN @startDate AND @endDate THEN 0
					WHEN YEAR(cn.contribution_date) &lt; YEAR(@startDate) THEN 2
					WHEN cn.contribution_date &lt; @startDate THEN 1
					END
	, cn.[contribution_id],
		 cn.[contribution_date],
		 [currency_type] = (SELECT TOP 1 lookup_value FROM [core_lookup] WHERE lookup_id = cn.[currency_type_luid]),
		 cn.[contribution_amount],
		 cn.[transaction_number],
		 cn.[transaction_id],
		 pl.[pledge_amount]
INTO #individual_giving
FROM #individual_funds ff
LEFT JOIN (
	SELECT 
		  cc.[person_id]
		, cc.[contribution_id]
		, cc.[contribution_date]
		, cc.[batch_id]
		, cc.[currency_type_luid]
		, cc.[account_number]
		, cc.[transaction_number]
		, cc.[transaction_id]
		, ccf.[fund_id]
		, [contribution_amount] = ccf.[amount]
		, ccf.[non_cash]
	FROM [ctrb_contribution] cc
	JOIN [ctrb_contribution_fund] ccf ON cc.[contribution_id] = ccf.[contribution_id]
	JOIN #individual_funds idf ON cc.[person_id] = idf.[person_id] AND ccf.[fund_id] = idf.[fund_id]
	WHERE cc.[contribution_date] &lt; @endDate
) AS cn ON cn.[person_id] = ff.[person_id] AND cn.[fund_id] = ff.[fund_id]
LEFT JOIN (
	SELECT 
		cpl.[person_id], cpl.[fund_id], [pledge_amount] = SUM(cpl.[amount])
	FROM [ctrb_pledge] cpl
	JOIN #individual_funds idf ON cpl.[person_id] = idf.[person_id] AND cpl.[fund_id] = idf.[fund_id]
	GROUP BY cpl.[person_id], cpl.[fund_id]
) AS pl ON pl.person_id = ff.[person_id] AND pl.fund_id = ff.[fund_id]

/*Populate selected_individuals table with family members for label creation*/
SELECT DISTINCT 
	cfm.[family_id]
	,cp.[person_id]
	, cp.[first_name]
	, cp.[nick_name]
	, [nick_names] = CONVERT(VARCHAR(200), '')
	, cp.[last_name]
	, [last_names] = CONVERT(VARCHAR(200), '')
	, [label] = CONVERT(VARCHAR(200), '')
	, cp.[marital_status]
	, cfm.[role_luid]
INTO #selected_individuals
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN #family_giving fg ON fg.[family_id] = cfm.[family_id]
	AND cp.[contribute_individually] = 0
	AND cp.[print_statement] = 1
		AND ((cfm.[role_luid] = 29 /*Adult*/ AND cp.[marital_status] = 10296/*Married*/)
			OR (cfm.[role_luid] = 29 /*Adult*/ AND cp.[marital_status] = 10298/*Couple*/)
			OR (cfm.[role_luid] = 10424 /*Other Adult*/ AND cp.[marital_status] = 10298/*Couple*/) )
	AND cp.[member_status] &lt;&gt; 10313 /*Deceased*/
ORDER BY cfm.[family_id];

/*Generate the combined name fields*/
IF EXISTS(SELECT * FROM #selected_individuals)
	EXEC dbo.cust_cotr_sp_mailingLabels;

/*Insert everyone left from family_giving into selected_individuals
	This would be everyone who is marked as giving to the family unit, but is not combined onto a label*/
INSERT INTO #selected_individuals
(	family_id
	, person_id
	, first_name
	, nick_name
	, nick_names
	, last_name
	, last_names
	, label
	, marital_status
	, role_luid
)
SELECT DISTINCT 
	cfm.[family_id]
	,cp.[person_id]
	, cp.[first_name]
	, cp.[nick_name]
	, [nick_names] = cp.[nick_name]
	, cp.[last_name]
	, [last_names] = cp.[last_name]
	, [label] = cp.[nick_name] + ' ' + cp.[last_name]
	, cp.[marital_status]
	, cfm.[role_luid]
FROM [core_person] cp
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
JOIN #family_giving fg ON cfm.[family_id] = fg.[family_id]
LEFT JOIN #selected_individuals si ON cp.[person_id] = si.person_id
WHERE si.person_id IS NULL
	AND cp.[member_status] &lt;&gt; 10313 /*Deceased*/
ORDER BY cfm.[family_id];

/*Insert into #selected_individuals those giving individually*/
INSERT INTO #selected_individuals
(
	[family_id]
	, [person_id]
	, [first_name]
	, [nick_name]
	, [nick_names]
	, [last_name]
	, [last_names]
	, [label]
	, [marital_status]
	, [role_luid]
)
SELECT DISTINCT 
	cfm.[family_id]
	,cp.[person_id]
	, cp.[first_name]
	, cp.[nick_name]
	, [nick_names] = cp.[nick_name]
	, cp.[last_name]
	, [last_names] = cp.[last_name]
	, [label] = cp.[nick_name] + ' ' + cp.[last_name]
	, cp.[marital_status]
	, cfm.[role_luid]
FROM [core_person] cp
JOIN #individual_giving fg ON fg.[person_id] = cp.[person_id]
JOIN [core_family_member] cfm ON cp.[person_id] = cfm.[person_id]
WHERE cp.[contribute_individually] = 1
	AND cp.[print_statement] = 1
	AND cp.[member_status] &lt;&gt; 10313 /*Deceased*/
ORDER BY cfm.[family_id];

/*Obtain Mailing address from the head of house*/
SELECT DISTINCT
	fg.[person_id]
	, fg.[fund_id]
	, fg.[fund_name]
	, fg.[fund_order]
	, fg.[selected_fund]
	, fg.[period]
	, fg.[contribution_id]
	, fg.[contribution_date]
	, fg.[currency_type]
	, fg.[contribution_amount]
	, fg.[transaction_number]
	, fg.[pledge_amount]
	, si.family_id
	, si.[nick_names]
	, si.last_names
	, si.label
	, ca.[street_address_1]
	, ca.[street_address_2]
	, ca.[city]
	, ca.[state]
	, ca.[postal_code]
FROM #family_giving fg
JOIN #selected_individuals si ON fg.[person_id] = si.[person_id]
JOIN [core_person_address] cpa ON cpa.[person_id] = si.[person_id]
JOIN [core_address] ca ON cpa.[address_id] = ca.[address_id]	
WHERE cpa.[address_type_luid] = 41 /*Main/Home*/
UNION
SELECT DISTINCT
	idg.[person_id]
	, idg.[fund_id]
	, idg.[fund_name]
	, idg.[fund_order]
	, idg.[selected_fund]
	, idg.[period]
	, idg.[contribution_id]
	, idg.[contribution_date]
	, idg.[currency_type]
	, idg.[contribution_amount]
	, idg.[transaction_number]
	, idg.[pledge_amount]
	, si.family_id
	, si.[nick_names]
	, si.last_names
	, si.label
	, ca.[street_address_1]
	, ca.[street_address_2]
	, ca.[city]
	, ca.[state]
	, ca.[postal_code]
FROM #individual_giving idg
JOIN #selected_individuals si ON idg.[person_id] = si.[person_id]
JOIN [core_person_address] cpa ON cpa.[person_id] = si.[person_id]
JOIN [core_address] ca ON cpa.[address_id] = ca.[address_id]	
WHERE cpa.[address_type_luid] = 41 /*Main/Home*/
ORDER BY [family_id], [selected_fund] DESC, [fund_order], [contribution_date]</CommandText>
