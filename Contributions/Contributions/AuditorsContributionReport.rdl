        </Field>
        <Field Name="fund_name">
          <DataField>fund_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="contribution_date">
          <DataField>contribution_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT        cfm.family_id, cc.person_id, ccf.amount, ccf.fund_id, cf.fund_name, cc.contribution_date
FROM            ctrb_contribution_fund AS ccf INNER JOIN
                         ctrb_contribution AS cc ON ccf.contribution_id = cc.contribution_id INNER JOIN
                         ctrb_fund AS cf ON ccf.fund_id = cf.fund_id INNER JOIN
                         core_person AS cp ON cp.person_id = cc.person_id INNER JOIN
                         core_family_member AS cfm ON cp.person_id = cfm.person_id
WHERE        (cc.contribution_date BETWEEN @StartDate AND @EndDate)
ORDER BY cc.contribution_date</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
