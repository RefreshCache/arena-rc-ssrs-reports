        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT *, A.street_address_1 +
		CASE WHEN A.street_address_2 IS NOT NULL AND A.street_address_2 &lt;&gt; '' THEN
			CHAR(10) + CHAR(13) + A.street_address_2
		ELSE '' END AS address
FROM
orgn_organization o
JOIN core_address a ON a.address_id = o.address_id
WHERE o.organization_id = @OrgId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@OrgId">
            <Value>=Parameters!OrgId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Pledges">
      <Fields>
        <Field Name="giving_unit_id">
          <DataField>giving_unit_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fund_name">
          <DataField>fund_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pledge_amount">
          <DataField>pledge_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>select P.giving_unit_id, F.fund_name, SUM(PL.amount) as pledge_amount
from ctrb_pledge PL
inner join core_person P
	on P.person_id = PL.person_id
inner join core_person RP
	on RP.giving_unit_id = P.giving_unit_id
inner join ctrb_fund F
	on F.fund_id = PL.fund_id
where PL.pledge_end_date &gt; @FromDate
and PL.pledge_begin_date &lt; DATEADD(d, 1, @ToDate)
and P.guid = @PersonGUID
group by P.giving_unit_id, F.fund_id, F.fund_name, F.fund_description, F.fund_order</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PersonGUID">
            <Value>=Parameters!PersonGUID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
