          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT cp.person_id,cp.nick_name+' '+cp.last_name AS full_name
FROM trip_mission_goer AS tmg
JOIN core_person AS cp ON cp.person_id=tmg.person_id
WHERE tmg.role_luid IN (@role)
AND tmg.mission_id IN (@mission)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@role">
            <Value>=Parameters!role.Value</Value>
          </QueryParameter>
