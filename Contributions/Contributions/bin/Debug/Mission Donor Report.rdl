      <Query>
        <DataSourceName>ArenaDB</DataSourceName>
        <CommandText>SELECT
tm.mission_id
,tm.name AS mission_name
,tm.cost_per_person
,cpg.person_id AS goer_person_id
,cpg.last_name AS goer_last_name
,dbo.cust_funct_fullName(cpg.person_id) AS goer_full_name
,cltr.lookup_value AS goer_role
,(CASE WHEN tmcg.anonymous='TRUE' THEN 0 ELSE cps.person_id END) AS supporter_person_id
,(CASE WHEN tmcg.anonymous='TRUE' THEN '_Anonymous' ELSE cps.last_name END) AS supporter_last_name
,(CASE WHEN tmcg.anonymous='TRUE' THEN 'Anonymous' ELSE dbo.cust_funct_fullName(cps.person_id) END) AS supporter_full_name
,tmcg.anonymous AS anonymous_support
,ca.street_address_1
,ca.street_address_2
,ca.state
,ca.city
,ca.postal_code
,cc.contribution_date
,cc.currency_amount AS contribution_amount
FROM
trip_mission AS tm
LEFT OUTER JOIN trip_mission_goer AS tmg ON tmg.mission_id=tm.mission_id
LEFT OUTER JOIN core_person AS cpg ON cpg.person_id=tmg.person_id
LEFT OUTER JOIN core_lookup AS cltr ON cltr.lookup_id=tmg.role_luid
LEFT OUTER JOIN trip_mission_contribution_goer AS tmcg ON tmcg.goer_id=tmg.goer_id 
LEFT OUTER JOIN ctrb_contribution AS cc ON cc.contribution_id=tmcg.contribution_id
LEFT OUTER JOIN core_person AS cps ON cps.person_id=cc.person_id AND tmcg.anonymous='FALSE'
LEFT OUTER JOIN core_person_address AS cpa ON cpa.person_id=cps.person_id AND cpa.primary_address='TRUE'
LEFT OUTER JOIN core_address AS ca ON ca.address_id=cpa.address_id
WHERE tm.mission_id IN (@mission)
AND tmg.person_id IN (@person)
AND tmg.role_luid IN (@role)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@mission">
            <Value>=Parameters!mission.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@person">
            <Value>=Parameters!person.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@role">
            <Value>=Parameters!role.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
